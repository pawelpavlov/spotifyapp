{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "inlineSourceMap": false,
    "inlineSources": false,
    "module": "amd",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedParameters": false,
    "sourceMap": true,
    "removeComments": true,
    "target": "es5",
    "outFile": "out.js"
  },
  "include": [ "Typescript/*"]
}